{
  "name": "naive-create-form",
  "version": "1.2.4",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lib-build": "vite build -c vite.config.lib.ts",
    "serve": "vite preview",
    "lint": "eslint .",
    "check": "npx taze -w",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "husky install",
    "test": "vitest",
    "publish": "npm run lib-build && np",
    "login": "npm login --registry https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.21",
    "@types/uuid": "^8.3.4",
    "@vicons/antd": "^0.11.0",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.24.0",
    "highlight.js": "^11.6.0",
    "husky": "^7.0.4",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.33.3",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "uuid": "^8.3.2",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0",
    "vitepress": "^1.0.0-alpha.16",
    "vitest": "^0.19.1",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13",
    "vue3-i18n-plugin": "^1.0.9",
    "vuex": "^4.0.2"
  },
  "peerDependencies": {
    "naive-ui": "^2.33.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "engines": {
    "node": ">=14"
  }
}
